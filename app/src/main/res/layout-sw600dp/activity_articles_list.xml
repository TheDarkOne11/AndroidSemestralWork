<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/newsListContainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".screens.articlesList.ActivityArticlesList">

    <fragment
        android:id="@+id/fragment"
        android:name="budikpet.cvut.cz.semestralwork.screens.articlesList.FragmentArticlesList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/articleContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="2">

    </android.support.constraint.ConstraintLayout>

</LinearLayout>
